@model IEnumerable<TripListingServiceModel>

@{
    ViewData["Title"] = "My Trips";
}

<h2>Signed Trips</h2>

@if (Model.Count() != 0)
{
    <h2>All Available Trips</h2>
    <div class="container">
        <div class="row">
            @for (int j = 0; j < Model.Count(); j++)
            {
                Html.RenderPartial("_TripListingPartial", Model.ElementAt(j));
            }
        </div>
    </div>
}
else
{
    <h3><i>You havent signed for any trips yet</i></h3>
}
