@model IEnumerable<AdminUserListingServiceModel>
@{
    ViewData["Title"] = "All";
}

<h2>All Users</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>Roles</th>
        </tr>
    </thead>
    <tbody>
        @foreach (AdminUserListingServiceModel userListing in Model)
        {
            <tr class="d-table-row">
                <td>@userListing.UserName</td>
                <td>@userListing.Email</td>
                <td>
                    <ul>
                        @foreach (string role in userListing.Roles)
                        {
                            <li>@role</li>
                        }
                    </ul>
                </td>
            </tr>
        }
    </tbody>
</table>