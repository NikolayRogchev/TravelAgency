@model CompanyTripListingServiceModel

<div id="container-trip-@Model.Id" class="panel panel-body panel-default col-md-4">
    <h3 class="text-center">@Model.Name</h3>
    <div class="col-md-6">
        <a onclick='trip.delete(@Model.Id)' class="btn btn-danger form-control">Delete</a>
    </div>
    <div class="col-md-6">
        <a asp-area="Company" asp-controller="Trips" asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-warning form-control">Edit</a>
    </div>
</div>